treeHit = 0

while treeHit <= 10:
    treeHit = treeHit+1
    
    print(" attacked %d tree  " %treeHit )
    if treeHit == 10:
        print("나무 넘어갑니다")
        break
# while문 직접 만들기
# 문자열 여러줄 저장하는 방법 """ ~~~ """
prompt = """ 
    1. Add
    2. Del
    3. List
    4. Quit
    
    Enter number: """

number = 0
while number != 4:
    print(prompt)
    number = int(input())
